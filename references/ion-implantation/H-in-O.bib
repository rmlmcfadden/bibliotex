@article{1970-Swint-NIM-80-134,
	title = {Energy loss of protons in gases},
	journal = {Nucl. Instrum. Methods},
	volume = {80},
	number = {1},
	pages = {134--140},
	year = {1970},
	issn = {0029-554X},
	doi = {10.1016/0029-554X(70)90308-3},
	url = {https://www.sciencedirect.com/science/article/pii/0029554X70903083},
	author = {J. B. Swint and R. M. Prior and J. J. Ramirez},
	% abstract = {Energy loss of protons in N2, air O2, Ne, Ar, Kr, CH4 and CO2 has been measured as a function of incident energy from 0.4 to 3.4 MeV. The incident beam of protons from a Van de Graaff accelerator was scattered from a thin gold foil into an isolated gas cell. A surface barrier solid state detector and a 512-channel pulse height analyzer were used to measure the energy of the protons after they had traversed the gas cell. The experimental results obtained have a probable error of 2–5%. Comparison of these results has been made with available experimental results, Whaling's stopping power curves and the Lindhard-Scharff theory.}
}

@Article{1975-Dose-ZPA-272-237,
