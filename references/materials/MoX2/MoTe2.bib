@article{1978-Clarke-PMB-38-121,
   author = {R. Clarke and E. Marseglia and H. P. Hughes},
   title = {A low-temperature structural phase transition in β-\ch{MoTe2}},
   journal = {Philos. Mag. B},
   volume = {38},
   number = {2},
   pages = {121--126},
   year  = {1978},
   publisher = {Taylor & Francis},
   doi = {10.1080/13642817808245670},
   URL = {https://doi.org/10.1080/13642817808245670},
   %eprint = {https://doi.org/10.1080/13642817808245670},
   abstract = {Abstract High-precision X-ray measurements on crystals of the layered dichalcogenide β-MoTe2 have revealed a structural transition at approximately 250 K. The transition from the monoclinic room-temperature form is accompanied by a discontinuous change of the monoclinic angle β from 93°55′ to 90°, with substantial thermal hysteresis and coexistence of high- and low-temperature phases. The low-temperature structure is the result of a shear deformation of the layers perpendicular to the c axis.}
}

@article{1979-Manolikas-PSSA-53-327,
   author = {Manolikas, C. and van Landuyt, J. and Amelinckx, S.},
   title = {Electron microscopy and electron diffraction study of the domain structures, the dislocation fine structure, and the phase transformations in β-\ch{MoTe2}},
   journal = {Phys. Status Solidi A},
   volume = {53},
   number = {1},
   pages = {327--338},
   doi = {10.1002/pssa.2210530138},
   url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/pssa.2210530138},
   %eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/pssa.2210530138},
   abstract = {Abstract Domain configurations in the β-phase of MoTe2 are analysed by electron diffraction and imaging techniques, and related with the α → β phase transition. Structural models are proposed for the orientation variants giving rise to ortho-, meta-, and para-type boundaries, and for translation variants occurring in β-MoTe2. The expected fine structure of dislocations is analysed, and the interaction behaviour of dislocation ribbons upon boundary crossing is discussed. From electron diffraction experiments it is concluded that the most frequently occurring twin boundaries are of the para-type. Finally it is demonstrated by electron diffraction that the displacive β → β′ transition occurs in a continuous manner whereby the β-angle of the monoclinic structure decreases continuously to 90°.},
   year = {1979}
}

@article{1987-Dawson-JPCSSP-20-6159,
   doi = {10.1088/0022-3719/20/36/017},
   url = {https://doi.org/10.1088/0022-3719/20/36/017},
   year = {1987},
   month = {dec},
   publisher = {IOP Publishing},
   volume = {20},
   number = {36},
   pages = {6159--6174},
   author = {W. G. Dawson and D. W. Bullett},
   title = {Electronic structure and crystallography of \ch{MoTe2} and \ch{WTe2}},
   journal = {J. Phys. C: Solid State Phys.},
   abstract = {Calculations of electronic structure have been undertaken using ab initio tight-binding (TB) method comparing the group VIb ditellurides WTe2 and MoTe2 in their various guises. The group VIb ditellurides show deviation from a simple band model which predicts semiconducting behaviour due to a trigonal prismatic crystal-field splitting of a filled nonbonding dz2 orbital. For WTe2 and beta -MoTe2 the metal atom is displaced from the centre of an octahedron of Te atoms, and metal-metal chains with bond lengths only slightly longer than the elemental metals run along the layers. The reduced Madelung term for this configuration compensates for loss of the lone dz2-based band and thus results in semi-metallic crystals. Mo but not W occurs with a trigonal prismatic coordination ( beta -MoTe2). beta -MoTe2 differs from WTe2 only slightly-in the stacking of layers; a low-temperature polymorph is believed to stack identically to WTe2.}
}

@article{2015-Cho-S-349-625,
   author = {Cho, Suyeon and Kim, Sera and Kim, Jung Ho and Zhao, Jiong and Seok, Jinbong and Keum, Dong Hoon and Baik, Jaeyoon and Choe, Duk-Hyun and Chang, K. J. and Suenaga, Kazu and Kim, Sung Wng and Lee, Young Hee and Yang, Heejun},
   title = {Phase patterning for ohmic homojunction contact in \ch{MoTe2}},
   volume = {349},
   number = {6248},
   pages = {625--628},
   year = {2015},
   doi = {10.1126/science.aab3175},
   publisher = {American Association for the Advancement of Science},
   abstract = {A key issue in fabricating transistors is making a good electrical contact to the semiconductor gate material. For two-dimensional materials, one route is through a phase transition that converts a hexagonally packed semiconductor phase into a distorted octahedrally packed metallic phase. Cho et al. show that laser heating of molybdenum telluride (MoTe2) achieves this conversion through the creation of Te vacancies. The phase transition improves charge carrier mobility while maintaining the low resistance necessary for improved transistor function.Science, this issue p. 625Artificial van der Waals heterostructures with two-dimensional (2D) atomic crystals are promising as an active channel or as a buffer contact layer for next-generation devices. However, genuine 2D heterostructure devices remain limited because of impurity-involved transfer process and metastable and inhomogeneous heterostructure formation. We used laser-induced phase patterning, a polymorph engineering, to fabricate an ohmic heterophase homojunction between semiconducting hexagonal (2H) and metallic monoclinic (1T{\textquoteright}) molybdenum ditelluride (MoTe2) that is stable up to 300{\textdegree}C and increases the carrier mobility of the MoTe2 transistor by a factor of about 50, while retaining a high on/off current ratio of 106. In situ scanning transmission electron microscopy results combined with theoretical calculations reveal that the Te vacancy triggers the local phase transition in MoTe2, achieving a true 2D device with an ohmic contact.},
   issn = {0036-8075},
   URL = {https://science.sciencemag.org/content/349/6248/625},
   %eprint = {https://science.sciencemag.org/content/349/6248/625.full.pdf},
   journal = {Science}
}

