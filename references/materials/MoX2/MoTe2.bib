@article{1970-Vellinga-JSSC-2-299,
   title = "Semiconductor to metal transition in \ch{MoTe2}",
   journal = "J. Solid State Chem.",
   volume = "2",
   number = "2",
   pages = "299--302",
   year = "1970",
   issn = "0022-4596",
   doi = "10.1016/0022-4596(70)90085-X",
   url = "http://www.sciencedirect.com/science/article/pii/002245967090085X",
   author = "Vellinga, M. B. and de Jonge, R. and Haas, C.",
   abstract = "MoTe2 undergoes a transition from a diamagnetic, semiconducting phase (α) to a metallic, pauliparamagnetic phase (β). The homogeneity region of the two phases lies between MoTe1.90 and MoTe1.99. The transition temperature is 820°C for Te-rich samples, and 880°C for Mo-rich samples. In order to avoid evaporation of Te, electrical resistivity measurements at high temperatures were carried out in a closed vessel, with the sample in equilibrium with its own vapour pressure. The properties of α- and β-MoTe2 are discussed in terms of a schematic energy level diagram."
}

