@article{1986-Bronsema-ZAAC-540-15,
   author = {K. D. Bronsema and J. L. De Boer and F. Jellinek},
   title = {On the structure of molybdenum diselenide and disulfide},
   journal = {Z. Anorg. Allg. Chem.},
   year = {1986},
   volume = {540},
   number = {9‐10},
   pages = {15--17},
   doi = {10.1002/zaac.19865400904},
   url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/zaac.19865400904},
   %eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/zaac.19865400904},
   abstract = {Abstract The crystal structure of MoSe2 has been refined from single‐crystal X‐ray diffraction data. MoSe2 is isotypic with hexagonal MoS2 (C7 type); a = 3.289(1) Å, c = 12.927(4) Å, z(Se) = 0.6210(3). It is pointed out that the structure of MoS2 is incorrectly given in many books, although it had already been correctly reported by Dickinson and Pauling [4]. Corrected interatomic distances are given; the trigonal prism about Mo is almost regular.}
}

