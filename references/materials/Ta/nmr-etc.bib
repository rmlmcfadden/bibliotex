@article{1976-Strobel-AP-9-39,
	author = "Str{\"o}bel, B. and L{\"a}uger, K. and B{\"o}mmel, H. E.",
	title = "Nuclear acoustic resonance in tantalum with low concentrations of hydrogen impurities",
	journal = "Appl. Phys.",
	year = "1976",
	month = "Jan",
	day = "01",
	volume = "9",
	number = "1",
	pages = "39--46",
	abstract = "Nuclear acoustic resonance (NAR) has been observed in tantalum single crystals with low concentrations of hydrogen (7.8 and 16 ppm) between 4.2 K and room temperature. The temperature dependence of the $\Delta$m=1 transition is reported for the first time. For the $\Delta$m=2 transition the temperature dependence of the linewidth is analysed in terms of the precipitation of a hydride phase in the Ta-H system. The differences in linewidth between virgin and pre-cooled runs can be interpreted by the effect of dislocations generated during the precipitation.",
	issn = "1432-0630",
	doi = "10.1007/BF00901907",
	url = "https://doi.org/10.1007/BF00901907"
}

