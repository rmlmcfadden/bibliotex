@article{1997-Cox-HI-106-57,
    author = "Cox, S. F. J. and Füchslin, R. and Meier, P. F. and Estle, T. L. and Cooke, D. W. and Morrobel‐Sosa, A. and Lichti, R. L. and Hitti, B. and Cottrell, S. P. and Chow, K. H. and Schwab, C.",
    title = "Muon level crossing resonance in niobium",
    journal = "Hyperfine Interact.",
    year = "1997",
    month = "Apr",
    day = "01",
    volume = "106",
    number = "1",
    pages = "57--62",
    abstract = "A useful guide to detecting quadrupole level crossing resonance (QLCR) spectra is to search in the vicinity of BE{\backslash}cdot({\backslash}gamman/{\backslash}gamma{\_}$\mu$), where BE is the onset magnetic field for the decoupling of the quadrupole interaction, as seen in muon spin rotation linewidths. More detailed predictions of the positions and intensities of the resonances require numerical simulations taking account of the local geometry of the muon site. We present such simulations for muons adjacent to nuclei of spin 9/2 and demonstrate a pronounced dependence on the anisotropy of the quadrupole coupling tensor. Simulations for the specific cases of muons located at the octahedral and tetrahedral interstices in niobium metal are compared with the experimentally detected spectrum in a polycrystalline sample.",
    issn = "1572-9540",
    doi = "10.1023/A:1012661017894",
    url = "https://doi.org/10.1023/A:1012661017894"
}

