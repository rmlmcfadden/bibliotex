@article{2015-Uzawa-IEEETAS-25-2401005,
	author = {Uzawa, Y. and Fujii, Y. and Gonzalez, A. and Kaneko, K. and Kroug, M. and Kojima, T. and Miyachi, A. and Makise, K. and Saito, S. and Terai, H. and Wang, Z.},
