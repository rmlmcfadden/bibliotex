@article{1990-DiLeo-JLTP-78-41,
	author = "Di Leo, R. and Nigro, A. and Nobile, G. and Vaglio, R.",
	title = "Niboium-titanium nitride thin films for superconducting rf accelerator cavities",
	journal = "J. Low Temp. Phys.",
	year = "1990",
	month = "Jan",
	day = "01",
	volume = "78",
	number = "1",
	pages = "41--50",
	abstract = "The development of sputtering-magnetron techniques makes possible the manufacturing of thin-film coated rf cavities for high-energy accelerators of various superconducting alloys or compounds. In this context high-quality (Nb1−xTix)N films have been produced by dc magnetron sputtering and carefully characterized. These films, for x≤0.5, exhibit a critical temperature Tc as high as NbN (Tc=17 K) but show a markedly lower normal state resistivity. The calculated BCS surface impedance of the films is also lower than for NbN. The overall properties of the (Nb1−xTix)N films are compared with those of other superconductors, and the suitability of this material for the production of superconducting sputter-coated copper cavities for particle physics accelerators is demonstrated. The potential interest of (Nb1−xTix)N films for superconducting electronics is also briefly outlined.",
	issn = "1573-7357",
	doi = "10.1007/BF00682108",
	url = "https://doi.org/10.1007/BF00682108"
}

@article{1991-Yamamoto-PC-185-2719,
	title = {High pressure synthesis of {B1}-type solid solutions \ch{Nb_{1-x}M_{x}N} (\ch{M} = \ch{Ga}, \ch{Ti})},
	journal = {Physica C},
	volume = {185--189},
	pages = {2719--2720},
	year = {1991},
	issn = {0921-4534},
	doi = {10.1016/0921-4534(91)91480-R},
	url = {https://www.sciencedirect.com/science/article/pii/092145349191480R},
	author = {T. Yamamoto and S. Kikkawa and M. Takahashi and Y. Miyamoto and F. Kanamaru},
	abstract = {High pressure syntheses were performed in both binary systems of c-NbN/GaN and c-NbN/TiN under 3GPa at 1400°C. B1-type solid solution Nb1−xMxN was obtained only in a limited compositional range up to X=0.15 in case of M=Ga, but in a whole range of 0≤×≤1 for M=Ti. The lattice constants of both B1-type solid solutions decrease with increasing amount of additives. The Nb1−xGaxN solid solution exhibits a maximum Tc of 17.5K at X=0.05 where average number of valence electrons per atom NVE is 4.95 though the NVE value for maximum Tc in B1-type solid solution is normally 4.9 according to Matthias rule. Remarkable hystereses were observed in temperature dependences of diamagnetic susceptibility depending on Ga amount. Such hystereses were not observed in the Nb1−xTixN solid solutions. These results suggest that Ga atoms cause serious lattice distortion in B1-type Nb1−xGaxN(X<0.15) which can be prepared only under high pressure.}
}

@incollection{1996-Kikkawa-CTMCN-9-175,
	author = "Kikkawa, S. and Yamamoto, T. and Ohta, K. and Takahashi, M. and Kanamaru, F.",
	editor = "Oyama, S. T.",
	title = "Transition metal-based double nitrides",
	bookTitle = "The Chemistry of Transition Metal Carbides and Nitrides",
