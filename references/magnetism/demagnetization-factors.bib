@article{2002-Aharoni-PSSB-229-1413,
    author = {Aharoni, A.},
    title = {``Local'' Demagnetization in a Rectangular Ferromagnetic Prism},
    journal = {Phys. Status Solidi B},
    volume = {229},
    number = {3},
    pages = {1413--1416},
    keywords = {75.10.Hk, 75.60.Jp },
    doi = {https://doi.org/10.1002/1521-3951(200202)229:3<1413::AID-PSSB1413>3.0.CO;2-Q},
    url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/1521-3951%28200202%29229%3A3%3C1413%3A%3AAID-PSSB1413%3E3.0.CO%3B2-Q},
    % eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/1521-3951%28200202%29229%3A3%3C1413%3A%3AAID-PSSB1413%3E3.0.CO%3B2-Q},
    abstract = {Abstract A new kind of demagnetizing factors is suggested, that express the local field in a ferromagnetic sample. An analytic expression is given for the case of a general rectangular prism and is compared with a generalization of the ballistic demagnetizing factor. These two factors are found to be nearly the same in the particular case of thin magnetic films, and differ significantly only when the three sides of the prism are of the same order of magnitude.},
    year = {2002}
}

